<odoo>
    <data>
        <record model="ir.ui.view" id="nievecus_hr_publiser_license_tree">
            <field name="name">nievecus_hr_publiser_license_tree</field>
            <field name="model">nievecus_hr.publisher.license</field>
            <field name="arch" type="xml" >
                <tree>
                    <field name="name" string="Publisher Name"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="nievecus_hr_publiser_license_form">
            <field name="name">nievecus_hr_publiser_license_form</field>
            <field name="model">nievecus_hr.publisher.license</field>
                <field name="arch" type="xml">
                    <form>
                        <sheet>
                            <group>
                                <field name="name" string="Publisher Name"/>
                            </group>
                        </sheet>
                    </form>
                 </field>
             </record>

    <!-- actions opening views on models -->

        <record model="ir.actions.act_window" id="nievecus_hr_publiser_license_action">
            <field name="name">Publisher window</field>
            <field name="res_model">nievecus_hr.publisher.license</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>